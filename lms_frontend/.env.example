# Frontend (React CRA) environment variables
# NOTE: Do not commit a real .env file with secrets; copy this as `.env` locally.

# Frontend site URL (used for auth email redirects)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Backend base URL (REST) and WS URL (if your backend exposes websockets)
REACT_APP_BACKEND_URL=http://localhost:3001
REACT_APP_WS_URL=ws://localhost:3001

# Supabase project settings
# Get these from your Supabase Project Settings â†’ API
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# Optional feature flags (comma-separated or JSON)
# e.g., REACT_APP_FEATURE_FLAGS=chat,ai
REACT_APP_FEATURE_FLAGS=chat,ai

# Optional: change dev server port (CRA)
REACT_APP_PORT=3000
