# Frontend environment variables (do NOT commit actual secrets)
# Supabase - use anon public key only (never service role)
REACT_APP_SUPABASE_URL=http://localhost:54321
REACT_APP_SUPABASE_KEY=your_anon_public_key

# Backend API base
REACT_APP_BACKEND_URL=http://localhost:3001
REACT_APP_API_BASE=http://localhost:3001

# Frontend public URL (used for auth email redirect)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional WebSocket URL override (Supabase client handles Realtime by default)
REACT_APP_WS_URL=

# Environment and build flags
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags (comma-separated or JSON). Example: ai,chat
REACT_APP_FEATURE_FLAGS=ai,chat
REACT_APP_EXPERIMENTS_ENABLED=false
